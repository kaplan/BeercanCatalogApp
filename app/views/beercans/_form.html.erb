<%= form_for @can do | f | %>
  <fieldset>
    <%= f.label "url" %>
    <%= f.text_field :url %>
  </fieldset>
  <fieldset>
    <%= f.label "caption:" %>
    <%= f.text_field :caption %>
  </fieldset>
  <fieldset>
    <%= f.label "brewery" %>
    <%= f.text_field :brewery %>
  </fieldset>
  <fieldset>
    <%= f.label "year brewed" %>
    <%= f.text_field :year_brewed %>
  </fieldset>
  <%= f.submit %>
<% end %>
